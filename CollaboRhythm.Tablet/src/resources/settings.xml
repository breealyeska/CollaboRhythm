<settings xmlns="http://collaborhythm.org/application/settings">
	<mode>patient</mode>
	<useFileTarget>true</useFileTarget>
	<useTraceTarget>true</useTraceTarget>
	<useSyslogTarget>false</useSyslogTarget>
	<useGoogleAnalytics>false</useGoogleAnalytics>
	<logSourceIdentifier>CollaboRhythmDefault</logSourceIdentifier>
	<collaborationEnabled>false</collaborationEnabled>
	<iHAARTOnly>true</iHAARTOnly>
	<rtmpBaseURI>rtmfp://adrenaline.media.mit.edu</rtmpBaseURI>
	<adherenceVoidingEnabled>false</adherenceVoidingEnabled>
	<includeAdherenceVoidingMenuItem>false</includeAdherenceVoidingMenuItem>
	<deviceSimulatorEnabled>false</deviceSimulatorEnabled>
	<targetDate>2011-08-13T10:40:00-5:00</targetDate>

	<pluginSearchPaths>
		<pluginSearchPath>$APPLICATION_STORAGE_DIRECTORY$/plugins</pluginSearchPath>
		<pluginSearchPath>$APPLICATION_DIRECTORY$/plugins</pluginSearchPath>
	</pluginSearchPaths>

	<appGroups>
		<appGroup id="mainWidgets">
			<appDescriptors>
				<appDescriptor>collaboRhythm.plugins.schedule.controller::ScheduleAppController</appDescriptor>
			</appDescriptors>
		</appGroup>
		<appGroup id="buttonWidgets">
			<appDescriptors>
				<appDescriptor>collaboRhythm.plugins.healthCharts.controller::HealthChartsAppController</appDescriptor>
				<appDescriptor>collaboRhythm.plugins.messages.controller::MessagesAppController</appDescriptor>
				<appDescriptor>collaboRhythm.plugins.problems.HIV.controller::HIVAppController</appDescriptor>
			</appDescriptors>
		</appGroup>
	</appGroups>
</settings>